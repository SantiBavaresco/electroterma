{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext", "es2015"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./*"]
        }
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".next/types/**/*.ts",
        "app/soler&palau/TDA/page.tsx",
        "components/imgslider/imageslider.jsx",
        "public/svg/ondas01svg.tsx",
        "./src/*"
, "components/videoplayer/videoplayer.jsx"    ],
    "exclude": ["node_modules"]
}
